{
    "env": {
        "compiler": "bit.envs/compilers/react-typescript@3.1.44",
        "tester": "bit.envs/testers/jest@22.4.3"
    },
    "resolveModules": {
        "modulesDirectories": [
            "editor/node_modules",
            "webapp/node_modules"
        ]
    },
    "overrides": {
        "*": {
            "peerDependencies": {
                "react": "+",
                "react-dom": "+"
            }
        }
    },
    "componentsDefaultDirectory": "editor/src/components/{name}",
    "packageManager": "npm"
}