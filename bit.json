{
    "env": {
        "compiler": "bit.envs/compilers/react-typescript@3.1.44",
        "tester": "bit.envs/testers/jest@22.4.3"
    },
    "resolveModules": {
        "modulesDirectories": [
            "node_modules",
            "packages/common/node_modules",
            "packages/editor/node_modules",
            "packages/webapp/node_modules"
        ]
    },
    "overrides": {
        "organisms/*": {
            "peerDependencies": {
                "react": "+",
                "react-dom": "+"
            }
        }
    },
    "componentsDefaultDirectory": "packages/editor/src/components/{name}",
    "packageManager": "yarn"
}